{% extends 'base.html.twig' %}

{% block title %}Enterprise{% endblock %}

{% block body %}
    <h1>Enterprise</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ enterprise.id }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ enterprise.email }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ enterprise.password }}</td>
            </tr>
            <tr>
                <th>Company_name</th>
                <td>{{ enterprise.companyName }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ enterprise.logo }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ enterprise.country }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ enterprise.phone }}</td>
            </tr>
            <tr>
                <th>ContactName</th>
                <td>{{ enterprise.contactName }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('enterprise_index') }}">back to list</a>

    <a href="{{ path('enterprise_edit', {'id': enterprise.id}) }}">edit</a>

    {{ include('enterprise/_delete_form.html.twig') }}
